{
    "name": "paparee/rakaca",
    "description": "Package GUI for Aptika Services",
    "keywords": [
        "Paparee",
        "laravel",
        "rakaca"
    ],
    "homepage": "https://github.com/papa-ree/rakaca",
    "license": "MIT",
    "authors": [
        {
            "name": "Papa Ree",
            "email": "ricky.romdhoni@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "awssat/laravel-visits": "*",
        "illuminate/contracts": "^11.0||^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Paparee\\Rakaca\\": "src/",
            "Paparee\\Rakaca\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paparee\\Rakaca\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Paparee\\Rakaca\\RakacaServiceProvider"
            ],
            "aliases": {
                "Rakaca": "Paparee\\Rakaca\\Facades\\Rakaca"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}